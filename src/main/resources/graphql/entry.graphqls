type Entry {
    id: ID!
    description: String!
    url: String!
}

extend type Query {
    entryById(id: ID!): Entry
    entriesByTagId(tagId: ID!): [Entry]
}

type EntryPage implements Page{
    number: Int!
    size: Int!
    totalPages: Int!
    numberOfElements: Int!
    totalElements: Int!
    hasNext: Boolean!
    isLast: Boolean!
    content: [Entry]
}
extend type Query {
    entries(page: Int, size: Int): EntryPage
}

